{
  "name": "generator-jhipster-lnnglar",
  "version": "0.1.0",
  "description": "JHipster blueprint that scaffolds Ruoyi-Vue-Pro backend with Yudao Vue3 admin frontend.",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-8",
    "yudao",
    "vue3",
    "spring-boot",
    "mybatis"
  ],
  "homepage": "https://github.com/lnnglar/generator-jhipster-lnnglar",
  "bugs": {
    "url": "https://github.com/lnnglar/generator-jhipster-lnnglar/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lnnglar/generator-jhipster-lnnglar.git"
  },
  "license": "Apache-2.0",
  "author": "lnnglar blueprint authors",
  
  "bin": {
    "jhipster-lnnglar": "cli/cli.cjs"
  },
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./generators/server": "./generators/server/index.js",
    "./generators/client": "./generators/client/index.js", 
    "./generators/entity": "./generators/entity/index.js"
  },
  "files": [
    "cli",
    "generators",
    "!**/__*",
    "!**/*.snap",
    "!**/*.spec.?(c|m)js"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "prepare": "husky",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "prettier:check": "prettier --check \"{,**/}*.{js,mjs,cjs,ts,json,md,yml,yaml}\"",
    "prettier:format": "prettier --write \"{,**/}*.{js,mjs,cjs,ts,json,md,yml,yaml}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "vitest run",
    "update-snapshot": "vitest run --update",
    "vitest": "vitest"
  },
  "dependencies": {
    "degit": "^2.8.4",
    "generator-jhipster": "8.11.0"
  },
  "devDependencies": {
    "ejs-lint": "2.0.1",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.4.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.11",
    "vitest": "3.1.3",
    "yeoman-test": ">=8.2.0"
  },
  "engines": {
    "generator-jhipster": "8.11.0",
    "node": "^18.19.0 || >= 20.6.1"
  }
}
