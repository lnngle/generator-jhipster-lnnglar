package cn.iocoder.yudao.module.system.api.dto;

import lombok.Data;
import lombok.experimental.Accessors;

import javax.validation.constraints.*;
<% if (fields.some(f => f.type === 'BigDecimal')) { %>
import java.math.BigDecimal;<% } %>
<% if (fields.some(f => f.type === 'LocalDate')) { %>
import java.time.LocalDate;<% } %>
<% if (fields.some(f => f.type === 'LocalTime')) { %>
import java.time.LocalTime;<% } %>

/**
 * <%= entityClass %> Base VO，提供给添加、修改、详细的子 VO 使用
 * 如果子 VO 存在差异的字段，请不要添加到这里，影响 Swagger 文档生成
 *
 * @author <%= author %>
 */
@Data
@Accessors(chain = true)
public class <%= entityClass %>BaseVO {

<% fields.forEach(function(field, index) { %>
    /**
     * <%= field.comment || field.name %>
     */
<% if (field.required) { %>
    @NotNull(message = "<%= field.comment || field.name %>不能为空")<% } else if (field.type === 'String') { %>
    @Size(max = <%= field.maxLength || 255 %>, message = "<%= field.comment || field.name %>长度不能超过<%= field.maxLength || 255 %>个字符")<% } %>
    private <%= field.type %> <%= field.name %>;

<% }); %>
}